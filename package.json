{
  "name": "crupee-salary-meter",
  "version": "0.0.1",
  "description": "Application for Salary Information Management",
  "main": "main.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . Crupee-Salary-Info --overwrite --asar --platform=darwin --arch=x64 --icon=build/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Crupee-Salary-Info --overwrite --asar --platform=win32 --arch=x64 --icon=build/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=Crupee --version-string.FileDescription=Crupee --version-string.ProductName=\"Crupee Salary Info\"",
    "package-linux": "electron-packager . Crupee-Salary-Info --overwrite --asar --platform=linux --arch=x64 --icon=build/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Susant, Sudip & Rajesh",
  "license": "ISC",
  "devDependencies": {
    "electron": "^8.0.1",
    "electron-packager": "^14.2.1",
    "npm-check-updates": "^4.0.1",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "mkdirp": "^1.0.3",
    "mysql": "^2.18.1",
    "popper.js": "^1.16.1"
  }
}
